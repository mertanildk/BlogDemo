@model Entity.Concrete.Writer
@{
    ViewBag.Title = "EditWriter";
    Layout = "AdminLayout";
}

<form method="post">
    
    <label>ID</label>
    <input type="text" asp-for="WriterId" class="form-control" />
    <label>Writer Name</label>
    <input type="text" asp-for="WriterName" class="form-control" />
    <label>Writer About</label>
    <input type="text" asp-for="About" class="form-control" />
    <label>Writer Image</label>
    <input type="text" asp-for="Image" class="form-control" />
    <label>Writer Mail</label>
    <input type="text" asp-for="Mail" class="form-control" />
    <label>Writer Status</label>
    <input type="text" asp-for="Status" class="form-control" />
    
    <button class="btn btn-dark">Update</button>
</form>

@section scripts { }